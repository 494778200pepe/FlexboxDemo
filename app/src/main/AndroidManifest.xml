<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zitech.flexboxdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FlexDirectionAct"/>
        <activity android:name=".FlexWrapAct"/>
        <activity android:name=".JustifyContentAct"/>
        <activity android:name=".AlignItemsAct"/>
        <activity android:name=".AlignContentAct"/>
        <activity android:name=".LayoutOrderAct"/>
        <activity android:name=".LayoutFlexGrowAct"/>
        <activity android:name=".LayoutFlexShrinkAct"/>
        <activity android:name=".LayoutAlignSelfAct"/>
    </application>

</manifest>